                                 baseTableName="adscripcion_empleados"
                                 constraintName="fk_adscripcion_empleados_empleados_id"
                                 referencedColumnNames="id"
                                 referencedTableName="empleado"/>
        
        <addForeignKeyConstraint baseColumnNames="adscripcions_id"
                                 baseTableName="adscripcion_proveedor"
                                 constraintName="fk_adscripcion_proveedor_adscripcions_id"
                                 referencedColumnNames="id"
                                 referencedTableName="adscripcion"/>
        <addForeignKeyConstraint baseColumnNames="proveedors_id"
                                 baseTableName="adscripcion_proveedor"
                                 constraintName="fk_adscripcion_proveedor_proveedors_id"
                                 referencedColumnNames="id"
                                 referencedTableName="proveedor"/>
        
    </changeSet>
</databaseChangeLog>
